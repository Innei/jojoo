{
  "name": "jojoo",
  "version": "0.0.1",
  "description": "A utils for Jotai v2.",
  "author": "Innei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Innei/rollup-typescript-lib",
    "directory": "Innei/rollup-typescript-lib"
  },
  "files": [
    "**"
  ],
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./*": {
      "import": "./src/*"
    }
  },
  "publishConfig": {
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "types/index.d.ts",
    "exports": {
      ".": {
        "type": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./src": {
        "import": "./src"
      },
      "./src/*": {
        "import": "./src/*"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.prettierignore --write ",
      "eslint --cache"
    ]
  },
  "bump": {
    "before": [
      "npm run build"
    ],
    "publish": true,
    "changelog": true
  },
  "scripts": {
    "prebuild": "rm -rf lib && rm -rf esm",
    "build": "rollup -c rollup.config.ts --configPlugin swc3 --bundleConfigAsCjs",
    "prepackage": "rm -rf dist",
    "test": "vitest",
    "prepare": "husky install && cd example && pnpm i",
    "release": "bump"
  },
  "peerDependencies": {
    "immer": "^10.0.0",
    "jotai": "^2.0.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@innei/bump-version": "1.5.10",
    "@innei/eslint-config-react-ts": "0.11.1",
    "@innei/eslint-config-ts": "0.11.1",
    "@innei/prettier": "0.11.1",
    "@rollup/plugin-commonjs": "25.0.4",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-typescript": "11.1.2",
    "@sukka/listdir": "0.2.0",
    "@swc/core": "1.3.75",
    "@testing-library/jest-dom": "6.1.2",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "20.5.0",
    "@types/react": "^18.0.0",
    "@unocss/preset-wind": "0.55.1",
    "browserslist": "4.21.10",
    "dts-bundle-generator": "^8.0.1",
    "esbuild": "0.19.2",
    "husky": "8.0.3",
    "immer": "10.0.2",
    "jotai": "2.4.0",
    "jsdom": "22.1.0",
    "lint-staged": "14.0.0",
    "postcss": "8.4.28",
    "postcss-import": "15.1.0",
    "postcss-nested": "6.0.1",
    "prettier": "3.0.2",
    "react": "^18.0.0",
    "rollup": "3.28.0",
    "rollup-plugin-dts": "6.0.0",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-swc3": "0.9.1",
    "tsc-alias": "1.8.7",
    "tslib": "2.6.1",
    "typescript": "5.1.6",
    "unocss": "0.55.1",
    "vite": "4.4.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.34.1"
  }
}
